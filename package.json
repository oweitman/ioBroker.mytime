{
  "name": "iobroker.mytime",
  "version": "0.6.1",
  "description": "This adapter handle Time (eg: countdown,etc.)",
  "author": {
    "name": "oweitman",
    "email": "oweitman@gmx.de"
  },
  "homepage": "https://github.com/oweitman/ioBroker.mytime",
  "license": "MIT",
  "keywords": [
    "ioBroker",
    "template",
    "Smart Home",
    "home automation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oweitman/ioBroker.mytime"
  },
  "dependencies": {
    "@date-io/core": "^1.3.13",
    "@date-io/date-fns": "^2.10.6",
    "@iobroker/adapter-core": "^2.4.0",
    "@material-ui/pickers": "^3.2.10",
    "date-fns": "^2.16.1",
    "prop-types": "^15.7.2",
    "rrule": "^2.6.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@iobroker/adapter-react": "^1.6.0",
    "@iobroker/testing": "^2.4.2",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/gulp": "^4.0.8",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.17",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/sinon": "^9.0.10",
    "@types/sinon-chai": "^3.2.5",
    "axios": "^0.21.1",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.18.0",
    "eslint-plugin-react": "^7.22.0",
    "gulp": "^4.0.2",
    "mocha": "^8.2.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-build-hooks": "^1.1.0",
    "proxyquire": "^2.1.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-icons": "^4.1.0",
    "sinon": "^9.2.3",
    "sinon-chai": "^3.5.0",
    "typescript": "^4.1.3"
  },
  "parcel-plugin-build-hooks": {
    "buildStart": "echo \"parcel buildStart\"",
    "bundled": "iob upload mytime",
    "buildEnd": "echo \"parcel buildEnd\"",
    "buildError": "echo \"parcel buildError\""
  },
  "main": "main.js",
  "scripts": {
    "watch:parcel": "parcel admin/src/index.jsx -d admin/build",
    "build:parcel": "parcel build admin/src/index.jsx -d admin/build",
    "build": "npm run build:parcel",
    "test:js": "mocha --config test/mocharc.custom.json \"{!(node_modules|test)/**/*.test.js,*.test.js,test/**/test!(PackageFiles|Startup).js}\"",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:js && npm run test:package",
    "check": "tsc --noEmit -p tsconfig.check.json",
    "lint:client": "eslint admin/src --ext .js,.jsx --env browser --ignore-pattern rrule.js --fix",
    "lint:server": "eslint main.js lib --ext .js,.jsx --env node --ignore-pattern rrule.js --fix",
    "lint": "npm run lint:client && npm run lint:server"
  },
  "bugs": {
    "url": "https://github.com/oweitman/ioBroker.mytime/issues"
  },
  "readmeFilename": "README.md"
}
